<body>
  <div class="banner">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="banner-img d-block w-100" src="<%= theme.nav.banner[0].banner %>"
            alt="<%= theme.nav.banner[0].slogan %>">
          <div class="container">
            <div class="carousel-caption text-left">
              <h1><%= theme.nav.banner[0].slogan %></h1>
              <p><%= theme.nav.banner[0].description %></p>
              <p><a class="btn btn-lg btn-outline-light" href="#" role="button">了解更多</a></p>
            </div>
          </div>
        </div>
        <div class="carousel-item">
          <img class="banner-img d-block w-100" src="<%= theme.nav.banner[1].banner %>"
            alt="<%= theme.nav.banner[1].slogan %>">
          <div class="container">
            <div class="carousel-caption">
              <h1><%= theme.nav.banner[1].slogan %></h1>
              <p><%= theme.nav.banner[2].description %></p>
              <p><a class="btn btn-lg btn-outline-light" href="#" role="button">了解更多</a></p>
            </div>
          </div>
        </div>
