          <img class="banner-img d-block w-100" src="<%= theme.nav.banner[2].banner %>"
            alt="<%= theme.nav.banner[2].slogan %>">
          <div class="container">
            <div class="carousel-caption text-right">
              <h1><%= theme.nav.banner[2].slogan %></h1>
              <p><%= theme.nav.banner[2].description %></p>
              <p><a class="btn btn-lg btn-outline-light" href="#" role="button">了解更多</a></p>
            </div>
          </div>
        </div>
        <div class="carousel-item">
          <img class="banner-img d-block w-100" src="<%= theme.nav.banner[3].banner %>"
            alt="<%= theme.nav.banner[2].slogan %>">
          <div class="container">
            <div class="carousel-caption text-right">
              <h1><%= theme.nav.banner[3].slogan %></h1>
              <p><%= theme.nav.banner[3].description %></p>
              <p><a class="btn btn-lg btn-outline-light" href="#" role="button">了解更多</a></p>
            </div>
          </div>
        </div>
      </div>
      <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>

  </div>
