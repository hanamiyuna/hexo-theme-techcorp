<div class="container">
    <div class="text-center">
        <% for(const i in theme.navbar.menu) { %>
        <% var link = theme.navbar.menu[i] %>
        <% if (!link) continue %>
        <li class="nav-item">
            <a class="nav-link" href="<%- url_for(link) %>"><%= i %></a>
        </li>
        <% } %>
    </div>
</div>